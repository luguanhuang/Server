<?xml version="1.0" encoding="utf-8"?>


<VersionServer name="version" id="$server_id$">

	<Listen>
		<peer ip="0.0.0.0" port="24001" handler="clientlink" sendBufSize="0" recvBufSize="0"/>
		<peer ip="0.0.0.0" port="24002" handler="gmtoollink" sendBufSize="0" recvBufSize="0"/>
	</Listen>

	<!-- rolltype (day, hour) -->
	<LogType  file="1" console="0" rolltype="day"/>
	<LogLevel info="1" debug="0" warn="1" error="1" fatal="1"/>

	<RootPath dir="versionconf"/>
</VersionServer>
